    game.Loaded:Wait()
end
repeat wait() until game.Players.LocalPlayer
wait(0.25)
_G.AutoExecuter = true
loadstring(game:HttpGet("https://raw.githubusercontent.com/Giangplay/Slap_Battles/main/Slap_Battles.lua"))()
    ]])
end
end

